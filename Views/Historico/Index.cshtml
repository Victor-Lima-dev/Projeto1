@model List<HistoricoMateria> 

<div class="container">
 <h1 class="text-center">Central de Desempenho</h1>

<div class="listaItens-box">

 @foreach (var item in Model)
    {
    
    <div class="listaItens">

    <h3>@item.NomeMateria</h3>
    <p class= "listaItens-HistoricoMateria" ><strong>Quantidade de Quest√µes</strong> @item.QuantidadeQuestoesRespondidas</p>
    <p class= "listaItens-HistoricoMateria" > <strong>Acertos</strong> @item.QuantidadeQuestoesCorretas</p>
    <p class= "listaItens-HistoricoMateria" ><strong>Erros</strong> @item.QuantidadeQuestoesErradas</p>
    <p class= "listaItens-HistoricoMateria" ><strong>Taxa de Aproveitamento</strong> @item.PorcentagemAcerto</p>

    <a asp-action="procurarPorMateria" asp-controller="ListaRegistros" asp-route-materia="@item.NomeMateria" class="btn btn-primary">Detalhes</a>

    </div>
    
}


</div>

</div>
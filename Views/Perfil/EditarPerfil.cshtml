@model UsuarioBancoDados

<form asp-action="EditarPerfil" method="post">

    <div>
        <label for="">Nome</label>
        <input asp-for="Nome" type="text" />
    </div>

    <div>
        <label for="">Email</label>
        <input asp-for="Email" type="text" />
    </div>

    <div>
        <label for="">Profissao</label>
        <input asp-for="Profissao" type="text" />
    </div>

    <div>
        <label for="">Objetivo</label>
        <input asp-for="Objetivo" type="text" />
    </div>

        <input asp-for="UsuarioGuidId" type="hidden" value="@Model.UsuarioGuidId" />

         <input asp-for="UsuarioBancoDadosId" type="hidden" value="@Model.UsuarioBancoDadosId" />
    

@* submit *@

    <div>
        <input type="submit" value="Salvar" />
    </div>




</form>